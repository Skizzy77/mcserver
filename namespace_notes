Global Volatile Namespace Hierarchy \
	| Modules \
	    | ObjectManager \
	        | Resources
	        | Events
	    | YAMLManager \
	        | Resources
	        | Events

Global Namespace Hierarchy \
	| Logging \
		| Format -> "%ident%:%compname% [%level%]: %msg%"
		| DebugLevel -> 1
	| Resources \
		| KernelName -> "MCKernel"
		| Owner -> ArrayList<String>{"fffaf","_Broadwell"}
		| WorkingDirectory -> String "/home/michael/SpigotCache/plugins/ServerKernel"
	| Manager \
	    | YAML \
	        | ModuleFolder -> String "Managers/YAML/Modules/"
	| Permissions -> ["Guest", "Privileged", "Developer", "Owner"]
	| Commands \
		| AFK \
			| Permission -> -1 (all)

Kernel module registry in Volatile Namespace \
	| Resources \
		| <options>
	| Events \
	    | <objects with wait/notifyAll>
	
